{
  "name": "timetrack",
  "author": {
    "name": "Joshua Wootonn",
    "email": "joshuawootonn@gmail.com",
    "url": "http://www.joshuawootonn.com"
  },
  "description": "Time Managment & Analytics",
  "version": "3.0.14",
  "url": "http://www.joshuawootonn.com",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "electron-log": "^3.0.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "exceljs": "^1.6.2",
    "formik": "^1.0.1",
    "is-electron": "^2.1.0",
    "keytar": "^4.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "normalizr": "^3.2.4",
    "react": "^16.4.1",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-simple-keyboard": "^1.21.3",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "yup": "^0.25.1"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "nf start -p 3000",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react.js",
    "react-start": "cross-env BROWSER=none react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint --fix .",
    "build": "react-scripts build",
    "pack": "npm run build && build --linux --win",
    "dist": "npm run build && build --linux  --ia32 --x64 --win -p always",
    "dist-test": "npm run build && build --ia32 --x64 --win -p always",
    "postinstall": "install-app-deps"
  },
  "build": {
    "appId": "com.electron.timetrack",
    "win": {
      "target": "nsis",
      "icon": "build/favicon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/favicon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "joshuawootonn",
        "repo": "time-track",
        "publishAutoUpdate": true
      }
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.5",
    "coveralls": "^3.0.2",
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.11.1",
    "jest-enzyme": "^7.0.1",
    "jsdom": "^13.0.0",
    "moxios": "^0.4.0",
    "prettier": "^1.18.2",
    "react-testing-library": "^5.3.1",
    "redux-mock-store": "^1.5.3",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!/src/routes",
      "!src/index.js",
      "!src/electron-starter.js",
      "!src/electron-wait-react.js",
      "!src/registerServiceWorker.js",
      "!src/**/styles.js"
    ]
  }
}
